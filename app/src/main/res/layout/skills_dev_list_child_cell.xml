<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#636661">

    <TextView
        android:id="@+id/skills_dev_list_child_cell_tv_role"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="SPOC Name"
        android:textColor="#ffffff"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/skills_dev_list_child_cell_tv_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="9629776226"
        android:textColor="#20b2e8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skills_dev_list_child_cell_tv_role" />

    <TextView
        android:id="@+id/skills_dev_list_child_cell_tv_address_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:linksClickable="true"
        android:text="Show Address"
        android:textColor="#20b2e8"
        app:layout_constraintBottom_toBottomOf="@+id/skills_dev_list_child_cell_tv_number"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/skills_dev_list_child_cell_tv_number" />

    <TextView
        android:id="@+id/skills_dev_list_child_cell_tv_codevalue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="EWRD"
        android:textColor="#ffffff"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/skills_dev_list_child_cell_tv_codekey"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/skills_dev_list_child_cell_tv_codekey"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:text="Code:"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="@+id/skills_dev_list_child_cell_tv_role"
        app:layout_constraintEnd_toStartOf="@+id/skills_dev_list_child_cell_tv_codevalue"
        app:layout_constraintTop_toTopOf="@+id/skills_dev_list_child_cell_tv_role" />
</android.support.constraint.ConstraintLayout>